{
  "name": "generator-ts-lambda",
  "version": "0.0.1",
  "description": "Generator for building AWS Lambda functions with Typescript",
  "main": "./lib/index.js",
  "scripts": {
    "prebuild": "npm run lint && rm -rf generators/*",
    "build": "tsc",
    "postbuild": "cpx \"src/*/templates/**/*\" generators \\\n&& cpx \"src/*/templates/**/.*\" generators",
    "lint": "tslint -c tslint.json -e 'node_modules/**/*' '**/*.ts'",
    "start": "node ./lib/index.js",
    "test": "NODE_ENV=test nyc mocha --require source-map-support/register --require ts-node/register --recursive './src/**/*.tests.ts'"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.15",
    "@types/yeoman-generator": "^3.0.1",
    "chai": "^4.2.0",
    "cpx": "^1.5.0",
    "mocha": "^5.2.0",
    "nyc": "^13.0.1",
    "prettier": "^1.14.3",
    "source-map-support": "^0.5.9",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.2.1"
  },
  "dependencies": {
    "yeoman-generator": "^3.2.0"
  }
}
